@model WebAnimalPassport.Models.Data.Vaccination.Vaccination
@{
    ViewData["Title"] = "Home Page";
}
<form method="post" asp-action="" enctype="multipart/form-data">
    <input asp-for="@Model.Id" hidden/>
    <div class="form-group row">
        <h2 class="text-dark pl-3">
            Информация о вакцинация
        </h2>
        <div class="row">
            <div class="col-8">
                <div class="form-group row">
                    <div class="col-4 my-1">
                        <label asp-for="PhotoPath"></label>
                    </div>
                    <div class="col-8 my-1">
                        <img src="PhotoPath"/>
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col-4 my-1">
                        <label asp-for="Type"></label>
                    </div>
                    <div class="col-8 my-1">
                        @Model.Type
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col-4 my-1">
                        <label asp-for="Series"></label>
                    </div>
                    <div class="col-8 my-1">
                        @Model.Series
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col-4 my-1">
                        <label asp-for="DoctorName"></label>
                    </div>
                    <div class="col-8 my-1">
                        @Model.DoctorName
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col-4 my-1">
                        <label asp-for="StartDate"></label>
                    </div>
                    <div class="col-8 my-1">
                        @Model.StartDate
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col-4 my-1">
                        <label asp-for="EndDate"></label>
                    </div>
                    <div class="col-8 my-1">
                        @Model.EndDate
                    </div>
                </div>
            </div>
            <div class="col-8">
                <div class="form-group row offset-4">
                    <a asp-action="Index" class="btn btn-dark w-100 pt-1">
                        <i class="fa-solid fa-arrow-left" style="color: #ffffff;"></i>
                        Back
                    </a>
                </div>
            </div>
        </div>
    </div>

</form>
@section Scripts{
    @{
        <partial name="_ValidationScriptsPartial" />
    }
}

